    "process-submodules": "node scripts/build.js",
    "watch": "node scripts/watch-simple.js"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.3",
    "katex": "^0.16.9",
    "marked": "^11.1.1",
    "ts-fsrs": "^4.1.0"
  },
  "devDependencies": {
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "vite": "^5.0.11"
  }
