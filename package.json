{
  "name": "flashcards",
  "version": "1.0.0",
  "description": "Browser-based spaced repetition flashcards with ts-fsrs",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"node scripts/watch-simple.js\"",
    "dev:no-watch": "vite",
